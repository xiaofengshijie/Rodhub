ReplicatedStorage:WaitForChild("GameData"):WaitForChild("LatestRoom"):GetPropertyChangedSignal("Value"):Connect(updateRemovals)

-- 添加删怪功能
local entityDeletionLoop
local function startEntityDeletion()
    if entityDeletionLoop then
        task.cancel(entityDeletionLoop)
    end
    
    entityDeletionLoop = task.spawn(function()
        while true do
            pcall(function()
                -- 删除各种实体
                local entitiesToDelete = {
                    "Figure", "Seek", "Screech", "Dread", "A90", "Eyes", 
                    "BackdoorLookman", "Snare", "SideroomDupe", "Seek_Arm", 
                    "ChandelierObstruction", "Halt", "Rush", "Ambush", "Timetable",
                    "Glitch", "A60", "A120"
                }
                
                for _, entityName in ipairs(entitiesToDelete) do
                    local entity = Workspace:FindFirstChild(entityName)
                    if entity then
                        entity:Destroy()
                    end
                    
                    -- 检查子对象
                    for _, obj in ipairs(Workspace:GetDescendants()) do
                        if obj.Name == entityName then
                            obj:Destroy()
                        end
                    end
                end
            end)
            
            task.wait(0.5) -- 每0.5秒检查一次
        end
    end)
end

HackGroup:AddToggle("DeleteEntities", { 
    Text = "删除所有实体", 
    Default = false, 
    Callback = function(state)
        if state then
            startEntityDeletion()
            Library:Notify("实体删除已开启", 3)
        else
            if entityDeletionLoop then
                task.cancel(entityDeletionLoop)
                entityDeletionLoop = nil
            end
            Library:Notify("实体删除已关闭", 3)
        end
    end
})

