    "AlarmClock","RiftSmoothie","GweenSoda","GlitchCub","ElectricalKeyObtain"
}
local ITEM_MAP = {}
for _,v in ipairs(ITEM_SET) do ITEM_MAP[v]=true end

local ENTITY_NAME_MAP = {
    RushMoving = "Rush",
    AmbushMoving = "Ambush",
    A60 = "A-60",
    A120 = "A-120",
    BackdoorRush = "Backdoor Rush",
    SallyWindow = "Sally",
    JeffTheKiller = "Jeff",
    GrumbleRig = "Grumble",
    GiggleCeiling = "Giggle",
