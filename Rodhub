)

makeRoomESP("‰øùÈô©‰∏ù",
    function(o)
        if o.Name == "FuseObtain" then
            return o
        end
    end,
    "‰øùÈô©‰∏ù",
    Color3.fromRGB(0, 255, 0)
)

